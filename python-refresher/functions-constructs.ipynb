    "def intersect(seq1, seq2):\n",
    "    res = [] # Start empty\n",
    "    for x in seq1: # Scan seq1\n",
    "        if x in seq2: # Common item?\n",
    "            res.append(x) # Add to end\n",
    "    return res\n",
    "\n",
    "x = intersect([1, 2, 3], (1, 4)) # Mixed types\n",
    "# Saved result object\n",
    "x"
   ]
